<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data>

        <!-- Tree -->
        <record id="property_tag_view_tree" model="ir.ui.view">
            <field name="name">property_tag_view_tree</field>
            <field name="model">estate.property.tag</field>
            <field name="arch" type="xml">
                <tree string="property_tag_tree">
                    <field name="name"/>
                </tree>
            </field>
        </record>
        
        <!-- Form -->
        <record id="property_tag_view_form" model="ir.ui.view">
            <field name="name">property_tag_view_form</field>
            <field name="model">estate.property.tag</field>
            <field name="arch" type="xml">
                <form string="property_tag_form">
                    <sheet>
                        <group>
                            <field name="name"/>
                            
                        </group>
                    </sheet>
                </form>
            </field>
        </record>


    </data>
</odoo>